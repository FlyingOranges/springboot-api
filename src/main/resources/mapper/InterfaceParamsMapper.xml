    <!-- S 批量插入 -->
    <insert id="createParamsList" keyProperty="id" useGeneratedKeys="true"
            parameterType="com.vue.api.entity.InterfaceParamsEntity">
        INSERT INTO `api_interface_params` (`interface_id`,`params_name`,`params_type`,
        `params_necessary`,`params_explain`,`created_at`,`updated_at`,`status`) VALUES

        <foreach collection="interfaceParamsEntityList" item="item" index="index" separator=",">
            (#{item.interfaceId},#{item.paramsName},#{item.paramsType},#{item.paramsNecessary},
            #{item.createdAt},#{item.updatedAt},#{item.status})
        </foreach>
    </insert>
    <!-- E 批量插入 -->

    <!-- S 批量插入 -->
    <insert id="createData" keyProperty="id" useGeneratedKeys="true"
