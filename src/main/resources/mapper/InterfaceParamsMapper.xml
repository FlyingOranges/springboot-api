        `params_necessary`,`params_explain`,`created_at`,`updated_at`,`status`) VALUES
            (#{interfaceId},#{paramsName},#{paramsType},#{paramsNecessary},
            #{createdAt},#{updatedAt},#{status})
    </insert>
